{
  "name": "professional-tour-of-heroes",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "shx": "shx",
    "start": "ng serve",
    "prebuild": "npm run clean:build",
    "build": "node scripts/ng-all.js ng build --prod",
    "test": "node scripts/required-tests.js",
    "test_watch": "ng test",
    "lint": "ng lint",
    "coverage": "ng test --code-coverage --watch false",
    "postcoverage": "npm run combine:coverage",
    "combine:coverage": "nyc -t coverage/project report --reporter=html --report-dir=coverage/combined",
    "update-webdriver": "node node_modules/protractor/node_modules/webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e --webdriver-update false",
    "protractor": "protractor e2e/protractor.conf.js",
    "protractor_watch": "watch-npm protractor",
    "protractor:dist": "protractor e2e/protractor.dist.js",
    "serve:dist": "http-server ./dist/apps",
    "clean:build": "shx rm -rf dist",
    "clean:coverage": "shx rm -rf coverage",
    "clean:spec-shots": "shx rm -rf ./spec-shots/local"
  },
  "watch": {
    "protractor": {
      "patterns": [
        "e2e/**/*.*",
        "src/**/*.*",
        "libs/**/*.ts",
        "testing/**/*.ts",
        "!**/*.spec.ts"
      ],
      "extensions": "*"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.14",
    "@angular/cdk": "^10.2.0",
    "@angular/common": "~10.0.14",
    "@angular/compiler": "~10.0.14",
    "@angular/core": "~10.0.14",
    "@angular/forms": "~10.0.14",
    "@angular/material": "^10.2.0",
    "@angular/platform-browser": "~10.0.14",
    "@angular/platform-browser-dynamic": "~10.0.14",
    "@angular/router": "~10.0.14",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.8",
    "@angular-devkit/build-ng-packagr": "~0.1000.8",
    "@angular/cdk": "^10.2.0",
    "@angular/cli": "~10.0.8",
    "@angular/compiler-cli": "~10.0.14",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "http-server": "^0.12.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-mocha-reporter": "^2.2.5",
    "ng-packagr": "^10.0.0",
    "nyc": "^15.1.0",
    "protractor": "~7.0.0",
    "protractor-image-comparison": "^3.9.0",
    "shx": "^0.3.2",
    "ts-node": "~8.3.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5",
    "watch-npm": "^1.0.1"
  }
}
